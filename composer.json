{
	"name": "skeeks/cms-cloudshop",
	"description": "Intgrations with Cloudshop",
	"keywords": ["yii", "app", "skeeks", "sx", "framework", "js", "css", "assets", "auto assets"],
    "homepage": "https://www.skeeks.com/",
	"type": "yii2-extension",
	"license": "BSD-3-Clause",
	"support": {
        "issues": "https://github.com/skeeks-semenov/yii2-cloudshop-api-client/issues",
        "source": "https://github.com/skeeks-semenov/yii2-cloudshop-api-client",
		"wiki": "https://cms.skeeks.com/",
		"authors": "https://skeeks.com"
    },
	"authors": [
        {
            "name": "SkeekS",
            "email": "support@skeeks.com",
            "homepage": "https://skeeks.com"
        },
        {
            "name": "Semenov Alexander",
            "email": "semenov@skeeks.com"
        }
    ],
	"require": {
		"skeeks/cms": "^5.6 || dev-master",
		"skeeks/yii2-cloudshop-api-client": "^1.0 || dev-master"
	},
    "autoload": {
        "psr-4": {
            "skeeks\\cms\\cloudshop\\": "src/"
        }
    },
    "extra": {
        "config-plugin": {
            "console": [
                "src/config/common.php",
                "src/config/console.php"
            ],
            "web": [
                "src/config/common.php",
                "src/config/web.php"
            ]
        }
    }
}
